{
  "name": "fastify-ts-template",
  "version": "0.0.0",
  "description": "This is a boiler plate for a microservice using fastify framework",
  "main": "dist/index.js",
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest -c jest.config.ts --coverage",
    "start": "ts-node src/index.ts --transpile-only",
    "dev": "nodemon",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint-fix"
    ]
  },
  "keywords": [],
  "author": "Ebuka Umeokonkwo",
  "license": "ISC",
  "dependencies": {
    "awilix": "~4.3.4",
    "dotenv": "~10.0.0",
    "fastify": "~3.18.1",
    "fastify-autoload": "~3.8.0",
    "fastify-cors": "~6.0.2",
    "fastify-plugin": "~3.0.0",
    "fastify-sensible": "~3.1.0",
    "fastify-swagger": "~4.8.3",
    "mongoose": "~5.13.2",
    "redis": "~3.1.2"
  },
  "devDependencies": {
    "@types/http-errors": "~1.8.0",
    "@types/jest": "~26.0.23",
    "@types/node": "~16.0.0",
    "@types/redis": "~2.8.30",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "concurrently": "~6.2.0",
    "eslint": "~7.30.0",
    "eslint-plugin-radar": "~0.2.1",
    "eslint-plugin-simple-import-sort": "~7.0.0",
    "fastify-blipp": "~3.1.0",
    "fastify-tsconfig": "~1.0.1",
    "husky": "~7.0.0",
    "jest": "~27.0.6",
    "lint-staged": "~11.0.1",
    "mongodb-memory-server": "~7.2.1",
    "nodemon": "~2.0.12",
    "pino-pretty": "~5.1.0",
    "ts-jest": "~27.0.3",
    "ts-node": "~10.1.0",
    "typescript": "~4.3.5"
  }
}
